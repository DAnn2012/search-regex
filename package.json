	"eslintConfig": {
		"env": {
			"mocha": true,
			"node": true
		},
		"extends": [
			"wpcalypso/react",
			"plugin:flowtype/recommended"
		],
		"plugins": [
			"flowtype"
		],
		"rules": {
			"max-len": 0,
			"wpcalypso/jsx-classname-namespace": 0,
			"wpcalypso/import-no-redux-combine-reducers": 0
		},
		"parserOptions": {
			"ecmaFeatures": {
				"experimentalObjectRestSpread": true
			}
		}
	},
	"jest": {
		"modulePaths": [
			"./client"
		]
	},
	"apidoc": {
		"name": "Search Regex REST API",
		"version": "1.0.0",
		"description": "WordPress REST API documentation for Search Regex",
		"title": "Search Regex REST API",
		"url": "https://searchregex.com/wp-json",
		"header": {
			"title": "Introduction",
			"filename": "./api/header.md"
		},
		"footer": {
			"filename": "./api/footer.md"
		},
		"template": {
			"withCompare": false
		},
		"sampleUrl": null
	},
	"wordpress": {
		"supported": "5.6"
	}
